{
  "/addmachine": {
    "post": {
      "x-oai-controller": [{
        "file": "machine",
        "handler": "add"
      }],
      "responses": { "200": { "description": "返回成功" } }
    }
  },
  "/searchmachine": {
    "post": {
      "x-oai-controller": [{
        "file": "machine",
        "handler": "search"
      }],
      "responses": { "200": { "description": "返回成功" } }
    }
  }
}