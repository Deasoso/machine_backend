{
  "/addorder": {
    "post": {
      "x-oai-controller": [{
        "file": "order",
        "handler": "add"
      }],
      "responses": { "200": { "description": "返回成功" } }
    }
  },
  "/searchorder": {
    "post": {
      "x-oai-controller": [{
        "file": "order",
        "handler": "search"
      }],
      "responses": { "200": { "description": "返回成功" } }
    }
  }
}